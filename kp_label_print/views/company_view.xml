<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="res_company_inherit_product_label_name" model="ir.ui.view">
            <field name="name">res.company.inherit.product.label.nam</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='favicon']" position="after">
                    <field name="product_label_name"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>